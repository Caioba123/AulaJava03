1° Quesito

public class Main {

	public static void main(String[] args) {
		 Scanner tec = new Scanner(System.in);
	       
	       System.out.println("Vamos Converter");
	       System.out.println("Coloque a sua Idade");
	       int anos = tec.nextInt();
	       
	       System.out.println("Coloque a sua idade em Meses");
	       int meses = tec.nextInt();
	       
	       System.out.println("Coloque a sua idade em Dias");
	       int dias = tec.nextInt();
	       
	       
	       int idadeEmDias = (anos * 365) + (meses * 30) + dias;
	       
	       System.out.println("Sua idade em dias é" + idade);
			 

	}
}


2° Quesito

    System.out.println("Coloque a porcentagem do IPI:");
		         double IPI = tec.nextDouble();

		         
		         System.out.println("Coloque o Código da peça 1:");
		         int codigo1 = tec.nextInt();

		         System.out.println("Coloque o Valor da peça 1:");
		         int valor1 = tec.nextInt();

		         System.out.println("Coloque a Quantidade de peças 1:");
		         int quant1 = tec.nextInt();

		     
		         System.out.println("Coloque o Código da peça 2:");
		         int codigo2 = tec.nextInt();

		         System.out.println("Coloque o Valor da peça 2:");
		         int valor2 = tec.nextInt();

		         System.out.println("Coloque a Quantidade de peças 2:");
		         int quant2 = tec.nextInt();

		       
		         double valorTotal = (valor1 * quant1 + valor2 * quant2) * (IPI / 100 + 1);
		         System.out.println("Valor total a ser pago (com IPI): R$ " + valorTotal);

		         
		         if (valor1 != valor2) {
		             System.out.println("Aprovado, Valores Diferentes");
		         } else {
		             System.out.println("Reprovado, Valores iguais");
		         }
		     }
		 }


